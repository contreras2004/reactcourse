const CustomCarousel = ({ data }: { data: string[] }) => {
  const width = Dimensions.get('window').width;

  return (
    <Carousel
      loop
      width={width}
      height={width}
      autoPlay={true}
      data={data}
      scrollAnimationDuration={1000}
      onSnapToItem={(index) => console.log('current index:', index)}
      renderItem={({ item }) => <Card imageUrl={item} />}
    />
  );
};